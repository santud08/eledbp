{
    
    "page":{
        "name": "page",
        "in":"query",
        "required":false,
        "description": "page from where you fetch the details",
        "schema": {
          "type": "integer",
          "example":1
        }
    },
    "limit":{
        "name": "limit",
        "in":"query",
        "required":false,
        "description": "limit from upto you fetch the details",
        "schema": {
          "type": "integer",
          "example":10
        }
    },
    "type":{
        "name": "type",
        "in":"query",
        "required":false,
        "description": "type which type details you want",
        "schema": {
          "type": "string",
          "example":"newest/trailer",
          "default":""
        }
    }
}